Algoritmo Actividad_04
	
	// Escriba un programa en el cual se ingrese un número y mientras ese número sea mayor
	// de 10, se pedirá el número de nuevo.
	
	Definir n Como Real;
	
	Escribir "Por favor, ingrese un numero: ";
	Leer n;
	
	Mientras n > 10 Hacer
		
		Escribir "Por favor, ingrese un numero: ";
		Leer n;
		
	FinMientras;
	
FinAlgoritmo
